@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
   <!DOCTYPE html>

<html>
<head>
     <meta name="viewport" content="width=device-width" />
    <title>Constructo</title>
      <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
  <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
</head>
     <center><h1>Constructo</h1></center> 
<body>
    <div class="row">
        <div class="col-md-2"></div>
    <div class="col-md-4">
@if (@ViewBag.Photomess != null)
    { 
    <h3>@ViewBag.Photomess</h3> 
    }

        else
        {
        <h1>@ViewBag.s</h1>
             <img src="~/Content/fahad/@ViewBag.s"  width="600"  height="300" />
         <h3>Text in image for using MODI OCR</h3>
         <h4>@ViewBag.modi2</h4>
        <br />
}
    </div>
        <div class="col-md-4">
            <div class="panel panel-primary">
    <div class="panel-heading" style="background-color:red">Low quality Material Rates</div>
            <div class="panel panel-info">
      <div class="panel-heading" style="border-color:#0e0303">Bricks Nos:            @ViewBag.bricks</div>
                </div>
            <div class="panel panel-info">
      <div class="panel-heading" style="border-color:#0e0303">Cement Bags:           @(ViewBag.cmnt + ViewBag.LanternCement)</div>
                </div>
            <div class="panel panel-info">
      <div class="panel-heading" style="border-color:#0e0303">Sand Cubic feet:       @(ViewBag.snd + ViewBag.LanternSand)</div>
                </div>
            <div class="panel panel-info">
      <div class="panel-heading" style="border-color:#0e0303">Steel Kgs :            @ViewBag.LanternSteel</div>
                </div>
                <div class="panel panel-info">
      <div class="panel-heading" style="border-color:#0e0303">Crush CubicFeet :      @ViewBag.LanternCrush</div>
                </div>
    </div>
            <input type="button" title="Submit"class="btn btn-success" style="width:20%"  value="Confirm" onclick= "location.href='@Url.Action("MapConfirmDim", "Home", new { targe="_blank" })    '" />
        </div>
        <div class="col-md-2"></div>
        </div>
</body>
</html>


